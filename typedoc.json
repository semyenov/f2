{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@cqrs/federation-v2",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "theme": "default",
  "readme": "README.md",
  "includeVersion": true,
  "sort": ["source-order", "alphabetical"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Method",
    "Accessor"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/cqrs/federation",
    "NPM": "https://www.npmjs.com/package/@cqrs/federation-v2"
  },
  "sidebarLinks": {
    "Getting Started": "https://github.com/cqrs/federation#getting-started",
    "Migration Guide": "https://github.com/cqrs/federation/blob/main/MIGRATION.md",
    "Examples": "https://github.com/cqrs/federation/tree/main/src/examples"
  },
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/test-*.ts",
    "**/tests/**/*",
    "**/node_modules/**/*",
    "src/examples/**/*"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "excludeExternals": true,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/cqrs/federation/blob/{gitRevision}/{path}#L{line}",
  "gitRevision": "main",
  "gitRemote": "origin",
  "hideGenerator": false,
  "searchInComments": true,
  "searchInDocuments": true,
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "blockTags": [
    "@param",
    "@returns",
    "@throws",
    "@example",
    "@deprecated",
    "@since",
    "@see",
    "@author",
    "@version",
    "@todo",
    "@internal",
    "@beta",
    "@alpha",
    "@public",
    "@shareable",
    "@inaccessible",
    "@tag",
    "@external",
    "@requires",
    "@provides",
    "@override",
    "@key",
    "@packageDocumentation",
    "@template",
    "@category"
  ],
  "modifierTags": [
    "@override",
    "@internal",
    "@alpha",
    "@beta",
    "@experimental",
    "@sealed",
    "@virtual",
    "@readonly"
  ],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Entity Builders",
    "Federation",
    "Schema Processing",
    "Performance Optimizations",
    "Error Handling",
    "Experimental",
    "*",
    "Other"
  ],
  "groupOrder": ["Classes", "Interfaces", "Types", "Functions", "Variables", "*"],
  "markdownItOptions": {
    "mangle": false
  },
  "titleLink": "https://github.com/cqrs/federation",
  "tsconfig": "tsconfig.json"
}
